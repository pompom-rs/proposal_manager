    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.7",
    "vite": "^4.2.1",
